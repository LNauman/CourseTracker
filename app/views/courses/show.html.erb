<h1>Students in <%= @course.title %></h1>	

<table>
	<thead>
		<tr>
			<th>First Name</th>
			<th>Last Nauman</th>
		</tr>
	</thead>

	<tbody>
		<% @students.each do |student| %>
			<tr>
				<td><%= student.first_name %></td>
				<td><%= student.last_name %></td>
				<td>grade in here</td>
				<td><%= link_to 'Edit', '#' %></td>
			</tr>
		<% end %>
	</tbody>
</table>
