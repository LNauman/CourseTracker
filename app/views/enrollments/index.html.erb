<h1>My Enrollments</h1>
	<table>
		<thead>
			<tr>
				<th>Course</th>
				<th>Grade</th>
				<th>Semester</th>
			</tr>
		</thead>
		<tbody>
			<% @student_enrollments.each do |enrollment| %>
				<tr>
					<td><%= enrollment.course.title %></td>
					<td><%= enrollment.grade %></td>
					<td><%= link_to "#{enrollment.course.semester.name}", '#' %><td>
				</tr>
			<% end %>
		</tbody>
	</table>
	