<h1>My Enrollments</h1>
<table>
	<thead>
		<tr>
			<th>Course Title</th>
		</tr>
	</thead>
	<tbody>
		<% @course_titles.each do |title| %>
			<tr>
				<td><%= title %></td>
			</tr>
		<% end %>
	</tbody>
</table>
<table>
	<thead>
		<tr>
			<th>Grades</th>
		</tr>
	</thead>
	<tbody>
		<% @grades.each do |grade| %>
			<tr>
				<% if grade == nil %>
					<td><%= '---' %></td>
				<% else %>
					<td><%= grade %></td>
				<% end %>	
			</tr>
		<% end %>
	</tbody>
</table>
